package com.example.netutilssamples.entities;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;

public class University{
	public String	name;
	public String	location;
	public Date		foundationTime;
	public String	category;
	public Person	director;
	public List<Person>	pupils;
	
	public University(String name, Date foundationTime, String category,
			Person director, List<Person> pupils) {
		super();
		this.name = name;
		this.foundationTime = foundationTime;
		this.category = category;
		this.director = director;
		this.pupils = pupils;
	}
	
	public static List<University> generate(int count){
		List<University> result = new ArrayList<University>(count);
		
		for(int i = 0; i < count; i++){
			
		}
		
		return result;
	}
	
}
